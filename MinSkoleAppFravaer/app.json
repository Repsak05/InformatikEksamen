{
  "expo": {
    "name": "MinSkoleAppFravaer",
    "slug": "MinSkoleAppFravaer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.nmp05.MinSkoleAppFravaer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "react-native-nfc-manager"
    ],
    "extra": {
      "eas": {
        "projectId": "8437c6c9-8fa2-4537-8cff-5b46e3a0cdd2"
      }
    }
  }
}
